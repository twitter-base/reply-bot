  "scripts": {
    "dev": "node --unhandled-rejections=strict --trace-warnings -r dotenv/config index.js ; ",
    "sleep": "node --unhandled-rejections=strict --trace-warnings server.js ; ",
    "build": "babel ./ --out-dir lib/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./lib/lib,./lib/node_modules --copy-files ; rm -f lib/package.json; echo \"{ \\\"type\\\": \\\"commonjs\\\" }\" > lib/package.json ; ",
    "lint": "echo \"Error: no linter specified\" && exit 1 ; ",
    "lint-test": "npm run-script lint ; npm run-script test ; ",
    "start": "node --unhandled-rejections=strict --trace-warnings -r dotenv/config index.js ; ",
    "build-start-lib": "npm run-script build ; npm run-script start-lib ; ",
    "start-lib": "node --unhandled-rejections=strict --trace-warnings lib/index.js ; ",
    "start-slow": "npm run-script start-lib ; npm run-script sleep ; ",
    "test": "echo \"Error: no test specified\" && exit 1 ; ",
    "twitter": "node --unhandled-rejections=strict --trace-warnings -r dotenv/config twitter.js ; ",
    "twitter-autohook": "node --unhandled-rejections=strict --trace-warnings -r dotenv/config twitter-autohook.js ; ",
    "version": "echo $npm_package_version ; "
  },
