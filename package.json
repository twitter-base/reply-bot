{
  "name": "twitter-reply-bot",
  "type": "module",
  "version": "0.6.14",
  "description": "base for twitter reply bot using autohook",
  "main": "./lib/index.js",
  "module": "lib/index.js",
  "bin": {
    "reply-bot": "./bin/server.js",
    "twitter-reply-bot": "./bin/server.js"
  },
  "scripts": {
    "sleep": "sleep",
    "start": "node --unhandled-rejections=strict --trace-warnings ./bin/server.js",
    "start:runner": "node --unhandled-rejections=strict --trace-warnings ./bin/runner.js",
    "test": "node --unhandled-rejections=strict --trace-warnings ./test/test.js",
    "build": "",
    "cz": "cz",
    "commit": "commit",
    "semantic-release": "semantic-release",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file",
    "changelog:unreleased": "conventional-changelog --preset angular --output-unreleased",
    "lint": "eslint lib",
    "version": "json version -a < package.json",
    "reply-bot": "npm run start",
    "twitter-reply-bot": "npm run start"
  },
  "exports": {
    "import": "./lib/index.js",
    "default": "./lib/index.js",
    "reply-bot": "./lib/index.js",
    "twitter-reply-bot": "./lib/index.js",
    "bot": "./lib/index.js",
    "server": "./bin/server.js",
    "package": "./package.json",
    "package.json": "./package.json"
  },
  "files": [
    "/CHANGELOG.md",
    "/CODE_OF_CONDUCT.md",
    "/CONTRIBUTORS.md",
    "/LICENSE",
    "/NOTICE.LICENSE.md",
    "/README.md",
    "/bin/server.js",
    "/lib/index.js",
    "/package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twitter-base/reply-bot.git"
  },
  "keywords": [
    "accessibility",
    "transcription",
    "a11y",
    "text2png",
    "transcription",
    "yes",
    "spongebob",
    "image",
    "image generation",
    "generation",
    "image-generation",
    "move-file",
    "move",
    "file",
    "twitter",
    "twitter-autohook",
    "twitter autohook",
    "autohook",
    "twitter-lite",
    "twitter lite",
    "lite",
    "automation",
    "docker",
    "bot",
    "png",
    "robot",
    "auto",
    "respond",
    "responder",
    "auto-respond",
    "auto-responder",
    "auto responder",
    "activity",
    "stream",
    "activity stream",
    "oauth",
    "access",
    "token",
    "access token",
    "retweet",
    "tweet",
    "RT",
    "DM",
    "direct",
    "message",
    "direct message",
    "hook",
    "no",
    "keywords",
    "keyword",
    "search",
    "webhook",
    "stream",
    "notification",
    "logging",
    "sponge",
    "scribe",
    "spongescribe",
    "sponge scribe",
    "transcribe",
    "transliterate",
    "transliteration"
  ],
  "author": "dezren39",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/twitter-base/reply-bot/issues"
  },
  "homepage": "https://github.com/twitter-base/reply-bot/tree/main#readme",
  "dependencies": {
    "anglicize": "^1.0.0",
    "bleetify": "^1.0.2",
    "emoji-regex": "^9.0.0",
    "fs": "0.0.2",
    "get-port": "^5.1.1",
    "replace-special-characters": "^1.0.2",
    "runes": "^0.4.3",
    "timeout": "^0.2.1",
    "translitro": "^0.2.3",
    "twitter": "^1.7.1",
    "twitter-autohook": "^1.7.2",
    "twitter-lite": "^0.14.0",
    "twitter-text": "^3.1.0",
    "twitter-tweet": "^1.2.0",
    "unzalgo": "^2.1.2",
    "weird-to-normal-chars": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@commitlint/prompt-cli": "12.1.4",
    "@saithodev/semantic-release-backmerge": "1.2.1",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "8.2.0",
    "husky": "6.0.0",
    "json": "10.0.0",
    "prettier": "2.1.2",
    "semantic-release": "17.2.3",
    "sleep-atomic": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      },
      {
        "name": "dev",
        "prerelease": true
      }
    ]
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "CHANGELOG.md",
          "CODE_OF_CONDUCT.md",
          "CONTRIBUTORS.md",
          "LICENSE",
          "NOTICE.LICENSE.md",
          "README.md",
          "bin/server.js",
          "lib/index.js",
          "package.json"
        ]
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          "CHANGELOG.md",
          "CODE_OF_CONDUCT.md",
          "CONTRIBUTORS.md",
          "LICENSE",
          "NOTICE.LICENSE.md",
          "README.md",
          "bin/server.js",
          "lib/index.js",
          "package.json"
        ],
        "assignees": "dezren39",
        "addReleases": "bottom"
      }
    ],
    [
      "@saithodev/semantic-release-backmerge",
      {
        "branchName": "dev",
        "plugins": [
          [
            "@semantic-release/exec",
            {
              "successCmd": "echo 'Version in main is ${nextRelease.version}' > test.txt && git add test.txt"
            }
          ]
        ]
      }
    ]
  ],
  "publishConfig": {
    "access": "public"
  }
}
